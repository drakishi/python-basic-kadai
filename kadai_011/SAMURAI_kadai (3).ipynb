      "source": [
        "array = [\"水\", \"金\", \"地\", \"火\", \"木\", \"土\", \"天\", \"海\", \"冥\"]\n",
        "repeated_array = []\n",
        "\n",
        "count = 0\n",
        "while count < 2:  # 2回繰り返す\n",
        "    for item in array:\n",
        "        repeated_array.append(item)\n",
        "    count += 1  # 回数をカウント\n",
        "\n",
        "print(repeated_array)"
      ],
