      "source": [
        "# 台形の面積を計算するためのPythonコード\n",
        "\n",
        "# 与えられた値\n",
        "top_base = 10  # 上辺 (cm)\n",
        "bottom_base = 20  # 下辺 (cm)\n",
        "height = 5  # 高さ (cm)\n",
        "\n",
        "# 台形の面積の公式\n",
        "area = (top_base + bottom_base) * height / 2\n",
        "\n",
        "# 結果の表示\n",
        "print(f\"{area} cm²\")"
      ],
